<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <ThingDef ParentName="BuildingBase">
        <defName>PLA_BuildingTurretDrone</defName>
        <label>turret drone</label>
        <description>A drone with 5.8mm machine gun turret.</description>
        <graphicData>
            <texPath>Vehicles/TurretDrone/TurretDrone</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>2</drawSize>
        </graphicData>
        <statBases>
            <WorkToBuild>1000</WorkToBuild>
        </statBases>
        <tickerType>Normal</tickerType>
        <rotatable>true</rotatable>
        <costList>
            <Steel>320</Steel>
            <Chemfuel>60</Chemfuel>
            <ComponentIndustrial>4</ComponentIndustrial>
            <ComponentSpacer>1</ComponentSpacer>
        </costList>
        <designationCategory>Ship</designationCategory>
        <researchPrerequisites>
            <li>PLA_ResearchVehicle_Basic</li>
        </researchPrerequisites>
        <comps>
            <li Class="PLA.Vehicle.CompProperties_BuildingToVehicle">
                <vehicle>PLA_TurretDrone</vehicle>
            </li>
        </comps>
        <uiIconPath>Vehicles/TurretDrone/Icon_TurretDrone</uiIconPath>
    </ThingDef>

