	<ThingDef Name="Base23x115mmSovietBullet" ParentName="BaseBullet" Abstract="true">
		<graphicData>
			<texPath>Things/Projectile/Bullet_Big</texPath>
			<graphicClass>Graphic_Single</graphicClass>
