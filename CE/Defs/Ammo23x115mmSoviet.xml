		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageDef>Bullet</damageDef>
			<speed>206</speed>
			<dropsCasings>true</dropsCasings>
		</projectile>
	</ThingDef>

    <ThingDef ParentName="Base23x115mmSovietBullet">
      <defName>Bullet_23x115mmSoviet_FMJ</defName>
